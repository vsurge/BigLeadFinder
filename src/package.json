  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm run dev",
    "electron": "electron ./dist",
    "dev": "webpack-dev-server --history-api-fallback --progress --host 0.0.0.0",
    "devX": "webpack --progress --watch",
    "clear-dist": "rm -Rf ./dist/*",
    "copy-index": "cp ./electron/index.html ./dist/",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run"
